--Window
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Ike", "Ocean")




local Main = Window:NewTab("FE Trolling Scripts")
local MainSection = Main:NewSection("R6 Trolling Scripts")


--Reset
MainSection:NewButton("Reset", "Resets your character.", function()
	game.Players.LocalPlayer.Character.Head:Destroy()
if game.Players.LocalPlayer.Character.Humanoid.Health < 5 then 
    local deathmanok = game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart").position
    wait(1.5)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(deathmanok)
end
end)


--FE TITTY
MainSection:NewButton("FE Titty (HATS NEEDED)", "HATS - 5945909830, 11679229, 14463468", function()
	loadstring(game:HttpGet('https://raw.githubusercontent.com/Ike5195/FE-SCRIPTS/main/FE-Tid'))()
end)

